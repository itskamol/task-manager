# PostgreSQL connection URL
# Example: postgresql://user:password@localhost:5432/mydatabase?schema=public
DATABASE_URL=postgresql://user:pass@localhost:5432/taskdb?schema=public

# JWT Secret Key for authentication
JWT_SECRET=yourSuperSecretKey

# Port the application will run on
PORT=3000

# Telegram Bot Token (get this from BotFather on Telegram)
TELEGRAM_BOT_TOKEN=yourTelegramBotToken

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
